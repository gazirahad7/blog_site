<div class="container">
  <% allData.map(item => {%>
  <div class="row mt-2">
    <div class="col-md-3"></div>
    <div class="col-md-6">
      <h4>Title:<%= item.title %></h4>

      <% JSON.parse(item.images).map((i) =>{ %>
      <img src="/uploadImages/<%= i %> " alt="" height="150px" width="200px" />
      <% }) %>
    </div>
    <div class="col-md-3"></div>
  </div>

  <% }) %>
</div>
